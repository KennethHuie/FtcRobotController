<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <LynxUsbDevice name="Control Hub Portal" parentModuleAddress="173" serialNumber="(embedded)">
        <LynxModule name="Control Hub" port="173">
            <!-- Motors -->
            <RevRoboticsUltraplanetaryHDHexMotor name="FL_Motor" port="0"/>
            <RevRoboticsUltraplanetaryHDHexMotor name="RL_Motor" port="1"/>
            <RevRoboticsUltraplanetaryHDHexMotor name="FR_Motor" port="2"/>
            <RevRoboticsUltraplanetaryHDHexMotor name="RR_Motor" port="3"/>
        </LynxModule>
        <LynxModule name="Expansion Hub 2" port="2">
            <!-- Motors -->
            <TetrixMotor name="hsMotor" port="1"/>
            <TetrixMotor name="vsRightMotor" port="2"/>
            <TetrixMotor name="vsLeftMotor" port="3"/>
            <!-- Servo -->
            <Servo name="bucket" port="0"/>
            <Servo name="grabber" port="1"/>
            <Servo name="bucketWristLeft" port="3"/>
            <Servo name="bucketWristRight" port="4"/>
            <ContinuousRotationServo name="sweeper" port="5"/>
        </LynxModule>
    </LynxUsbDevice>
</Robot>